@article{Edmunds2006,
abstract = {Surprisingly little is known regarding the human mixing patterns relevant to the spread of close-contact infections, such as measles, influenza and meningococcal disease. This study aims to estimate the number of partnerships that individuals make, their stability and the degree to which mixing is assortative with respect to age. We defined four levels of putative at-risk events from casual (physical contact without conversation) to intimate (contact of a sexual nature), and asked university student volunteers to record details on those they contacted at these levels on three separate days. We found that intimate contacts are stable over short time periods whereas there was no evidence of repeat casual contacts with the same individuals. The contacts were increasingly assortative as intimacy increased. Such information will aid the development and parameterisation of models of close contact diseases, and may have direct use in outbreak investigations. {\textcopyright} 2006 Edmunds et al; licensee BioMed Central Ltd.},
