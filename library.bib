file = {:C$\backslash$:/Users/Tabita/Documents/Covid/Kalman/Uso en covid/short term forectast cameroon.pdf:pdf},
issn = {09600779},
journal = {Chaos, Solitons and Fractals},
keywords = {Basic reproduction number,COVID-19 Pandemic,Control measures,Mathematical models,Short-term forecasts,covid19,enkf},
mendeley-tags = {covid19,enkf},
pages = {110106},
