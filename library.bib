issn = {00361399},
journal = {SIAM Journal on Applied Mathematics},
keywords = {Machine learning,Stochastic analysis,Weak convergence},
number = {2},
pages = {725--752},
title = {{Mean field analysis of neural networks: A law of large numbers}},
volume = {80},
year = {2020}
}
